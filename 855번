#855ë²ˆ
a, b = map(int, input().split())
sum = 0
avg = 0
cnt = 0
if a<b:
    for i in range(a,b+1):
        if i%3 == 0 or i%5 == 0:
            sum += i
            cnt += 1
            avg = sum/cnt
    print('sum :',sum)
    print('avg : %.1f' %avg)
else:
    for i in range(b,a+1):
        if i%3 == 0 or i%5 == 0:
            sum += i
            cnt += 1
            avg = sum/cnt
    print('sum :',sum)
    print('avg : %.1f' %avg)
